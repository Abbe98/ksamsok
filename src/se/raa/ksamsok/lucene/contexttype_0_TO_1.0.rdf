<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
	xmlns="http://kulturarvsdata.se/ksamsok#" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xml:base="http://kulturarvsdata.se/resurser/ContextType">
	
	<ContextType rdf:ID="create">
		<contextLabel>Tillverkad</contextLabel>
		<description>Omfattar tillverkning, skapande etc. Tillverkad kan ersättas med Skapad, Skissad, Uppfunnen, Konstruerad, Modifierad eller Färdigställd.</description>
	</ContextType>
	
	<ContextType rdf:ID="find">
		<contextLabel>Funnen</contextLabel>
		<description>Omfattar hittande etc. Funnen kan ersättas med Påträffad.</description>
	</ContextType>
	
	<ContextType rdf:ID="use">
		<contextLabel>Brukad</contextLabel>
		<description>Omfattar bruk, aktivitet, ändringar etc. Brukad kan ersättas med Restaurerad, Boplatsaktivitet, Gravaktivitet eller Annan aktivitet.</description>
	</ContextType>
	
	<ContextType rdf:ID="explore">
		<contextLabel>Undersökt</contextLabel>
		<description>Omfattar undersökningar, analyser, konservering etc. Undersökt kan ersättas med Inventerad, Utgrävd eller Konserverad.</description>
	</ContextType>
	
	<ContextType rdf:ID="transact">
		<contextLabel>Deponerad</contextLabel>
		<description>Omfattar överföringar, gåvor, förvärv etc. Deponerad kan ersättas med Förvärvad eller Skänkt.</description>
	</ContextType>
	
	<ContextType rdf:ID="own">
		<contextLabel>Ägd</contextLabel>
		<description>Omfattar ägande. Inga andra valbara alternativ på etiketten för närvarande.</description>
	</ContextType>
	
	<ContextType rdf:ID="exist">
		<contextLabel>Belägen</contextLabel>
		<description>Omfattar belägenhet. Inga andra valbara alternativ på etiketten för närvarande.</description>
	</ContextType>
	
	<ContextType rdf:ID="reproduced">
		<contextLabel>Fotograferad</contextLabel>
		<description>Omfattar avbildning. Fotograferad kan ersättas med Förstorad.</description>
	</ContextType>

	<ContextType rdf:ID="destroyed">
		<contextLabel>Förstörd</contextLabel>
		<description>Omfattar händelser som avslutar objektets livscykel. Förstörd kan ersättas med Förlorad (inkluderar borttappad och stulen) eller Avslutad.</description>
	</ContextType>

</rdf:RDF>
